<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from UtilisateurService WSDL service -->

  <service name="Utilisateur" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws"/>
      <parameter name="wsdlServiceElement" value="UtilisateurService"/>
      <parameter name="schemaQualified" value="http://ws"/>
      <parameter name="wsdlServicePort" value="Utilisateur"/>
      <parameter name="className" value="ws.UtilisateurSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Utilisateur"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="lister" qname="operNS:lister" xmlns:operNS="http://ws" returnQName="retNS:listerReturn" xmlns:retNS="http://ws" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ws" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="lister"/>

  </service>
</deployment>
